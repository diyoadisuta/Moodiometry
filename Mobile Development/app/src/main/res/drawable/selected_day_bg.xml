<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <!-- Warna background (Hijau) -->
    <solid android:color="#4CAF50" />

    <!-- Efek bayangan atau garis tepi -->
    <stroke
        android:width="2dp"
        android:color="#388E3C" />

    <!-- Ukuran padding di dalam oval -->
    <padding
        android:left="8dp"
        android:top="8dp"
        android:right="8dp"
        android:bottom="8dp" />

    <!-- Efek ukuran oval -->
    <size
        android:width="60dp"
        android:height="60dp" />
</shape>
